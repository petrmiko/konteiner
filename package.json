{
  "name": "konteiner",
  "version": "0.2.2",
  "description": "Simple DI container for node.js applications",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:petrmiko/konteiner.git"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha --recursive './src/**/*.test.js'",
    "test:coverage": "nyc --reporter=lcovonly --report-dir=coverage npm run test && nyc report",
    "publish:coveralls": "node --eval \"require('fs').createReadStream(process.argv[1]).pipe(process.stdout)\" ./coverage/lcov.info | coveralls",
    "lint": "eslint . && tsc"
  },
  "keywords": [
    "di",
    "ioc",
    "dependency",
    "injection",
    "container",
    "nodejs"
  ],
  "author": "Petr Miko",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^10.14.13",
    "chai": "^4.2.0",
    "coveralls": "^3.0.5",
    "cross-env": "^5.2.0",
    "eslint": "^6.0.1",
    "husky": "^3.0.0",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^14.1.1",
    "sinon": "^7.3.2",
    "typescript": "^3.5.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "eslint .",
      "pre-push": "npm run test:coverage"
    }
  }
}
